#%Module 1.0 -*- tcl -*-
#
#  Module for use with 'environment-modules' package:
# 

set version [ file tail [ module-info name ] ]
set name tecplot

if { [info exists env(TECPLOT_ROOT)] } {
    set app_dir $env(TECPLOT_ROOT)
} else {
    if { [info exists env(KAUST_APPS_ROOT)] } {
	set app_dir $env(KAUST_APPS_ROOT)/TECPLOT
    } else {
	set app_dir /opt/share/TECPLOT
    }
}

source $env(KAUST_MODULES_ROOT)/common/common_setup.tcl
GeneralAppSetup

prepend-path PATH $app_dir/v$version.app/prefix/bin:$app_dir/v$version.app/bin


