#%Module 1.0 -*- tcl -*-
#
#  Module for use with 'environment-modules' package:
source [file dirname $::ModulesCurrentModulefile]/../../common/common_setup2.tcl
GeneralLibSetup 

# no conflicts
# no prereq 

prepend-path PATH $app_dir/bin
